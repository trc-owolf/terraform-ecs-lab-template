# Terraform
tfa='terraform apply'
tfat='terraform apply -target "$@"'
tff='terraform format'
tfi='terraform init'
tfmi='touch main.tf tf_variables.tf tf_outputs.tf tf_locals.tf tf_data.tf tf_versions.tf tf_providers.tf tf_variables.tf README.md CHANGELOG.md LICENSE.md'
tfp='terraform plan'
tfv='terraform validate'
tfwl='terraform workspace list'
tfws='terraform workspace select "$@"'
